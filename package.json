{
  "name": "foo",
  "version": "0.1.0",
  "description": "React Cellular Automata",
  "main": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesMcGuigan/react-cellular-automata"
  },
  "scripts": {
    "now-build": "next build",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "eslint": "eslint .",
    "tslint": "tslint --project tsconfig.json -c tslint.json -e '*.js' -t codeFrame; tslint --project tsconfig.json -c tslint.json -e '*.js' -t stylish",
    "data": "yarn tsc; yarn data:2x2 & yarn data:3x3 & yarn data:1x9 & yarn data:4x5",
    "data:2x2": "time ( seq 0 9 | xargs -L1 -P10 -t node --max-old-space-size=8192 ./dist/server/generateShapeStats.js --save -x 2 -y 2 --rule )",
    "data:3x3": "time ( seq 0 9 | xargs -L1 -P10 -t node --max-old-space-size=8192 ./dist/server/generateShapeStats.js --save -x 3 -y 3 --rule )",
    "data:1x9": "time ( seq 0 9 | xargs -L1 -P10 -t node --max-old-space-size=8192 ./dist/server/generateShapeStats.js --save -x 1 -y 9 --rule )",
    "data:4x5": "time ( seq 0 9 | xargs -L1 -P10 -t node --max-old-space-size=8192 ./dist/server/generateShapeStats.js --save -x 4 -y 5 --rule )",
    "tsc": "yarn tsc:clean; yarn tsc:build",
    "tsc:build": "tsc --build tsconfig.server.json",
    "tsc:watch": "tsc --build tsconfig.server.json -w",
    "tsc:clean": "rm -rvf ./dist"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@tensorflow/tfjs": "^1.1.2",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "autobind-decorator": "^2.4.0",
    "autoprefixer": "^9.5.1",
    "less": "^3.9.0",
    "lodash": "^4.17.11",
    "mathjs": "^5.10.3",
    "ndarray": "^1.0.18",
    "next": "^9.3.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rollup": "^1.12.4",
    "seedrandom": "~2.4.3",
    "styled-jsx-plugin-less": "^0.0.1-alpha.1",
    "styled-jsx-plugin-postcss": "^2.0.0",
    "styled-jsx-plugin-stylelint": "^0.1.0",
    "typescript": "^3.4.5",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.134",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.18",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-standard": "^4.0.0",
    "ghooks": "^2.0.4",
    "now": "^15.3.0",
    "prettier": "^1.17.1",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
